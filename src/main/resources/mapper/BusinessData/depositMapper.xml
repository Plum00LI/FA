<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ssaw.BusinessData.mapper.DepositMapper">
    <insert id="insertDeposit" parameterType="int">
        INSERT INTO users (depositId,fundId,outAccountId,inAccountId,directionOfMoney,
        businessDate,businessType,Money,Interest,endDate,flag,depositDesc) VALUES(#{depositId},#{fundId},#{outAccountId}
        ,#{inAccountId},#{directionOfMoney},#{businessDate},#{businessType},#{Money},#{Interest},#{endDate},#{flag},#{depositDesc})
    </insert>
    <delete id="deleteDeposit">
    delete from deposit where depositId=#{depositId}
    </delete>


    <select id="selectDeposit" resultType="com.ssaw.BusinessData.mapper.DepositMapper">
        select * from deposit
    </select>
</mapper>