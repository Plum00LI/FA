<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ssaw.ReportManagement.mapper.SecuritiesMarketMapper">
    <select id="selectSecuritiesMarket" resultType="com.ssaw.ReportManagement.entity.SecuritiesMarket">

    </select>
    <select id="selectProjectId" resultType="java.lang.Double">
        select projectId from valueStatistics where projectName = '资产净值'
    and valueStatistics.dateTime=#{dateTime} and fundId=#{fundId};
    </select>
</mapper>
